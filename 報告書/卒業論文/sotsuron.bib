@article{McLachlan,
author = {McLachlan, David S. and Blaszkiewicz, Michael and Newnham, Robert E.},
title = {Electrical Resistivity of Composites},
journal = {Journal of the American Ceramic Society},
volume = {73},
number = {8},
pages = {2187-2203},
keywords = {electrical conductivity, resistivity, percolation, composites, modeling},
doi = {10.1111/j.1151-2916.1990.tb07576.x},
url = {https://ceramics.onlinelibrary.wiley.com/doi/abs/10.1111/j.1151-2916.1990.tb07576.x},
eprint = {https://ceramics.onlinelibrary.wiley.com/doi/pdf/10.1111/j.1151-2916.1990.tb07576.x},
abstract = {Percolation and Bruggeman's effective media theories, as they apply to the electrical conductivity of composites, are reviewed, and a general effective media (GEM) equation, which combines most aspects of both percolation and effective media theories, is introduced. It is then shown that the GEM equation quantitatively fits electrical resistivity (conductivity) as a function of the volume fraction data for binary composites. The parameters used to fit the experimental data are the electrical resistivities of the two phases, the percolation threshold for the lower resistivity phase (\UTF{00F4}c), and an exponent t. Preliminary work, showing how the GEM equation can be used to model the piezoresistivity of composites by postulating that \UTF{00F4}c is a function of the independent variable, is also presented.},,
year = {1990}
}

